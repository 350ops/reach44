{
  "expo": {
    "newArchEnabled": true,
    "name": "Luna",
    "slug": "luna",
    "version": "1.0.0",
    "runtimeVersion": "1.0.0",
    "scheme": "luna",
    "experiments": {
      "tsconfigPaths": true
    },
    "web": {
      "output": "server"
    },
    "platforms": [
      "ios",
      "android"
    ],
    "plugins": [
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "merchant.com.mmdev13.luna",
          "enableGooglePay": true
        }
      ],
      "expo-router",
      "expo-font"
    ],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mmdev13.luna"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "softwareKeyboardLayoutMode": "pan",
      "predictiveBackGestureEnabled": false,
      "config": {
        "enableOnBackInvokedCallback": false
      },
      "navigationBarColor": "transparent",
      "navigationBarHidden": true,
      "package": "com.mmdev13.luna"
    },
    "extra": {
      "router": {
        "origin": "https://www.reach44.com"
      },
      "eas": {
        "projectId": "dda0ffb6-9ccc-430a-b6ff-d4cc9844c2a2"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/a0a0f625-0457-4ab7-a22f-509a24238a11"
    }
  }
}